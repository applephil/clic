<script type="text/template" id="login_form_template">

<div class="fb-login-button" data-width="750" data-height="200" data-max-rows="1" data-show-faces="true" data-onclick="facebookLogin()"></div>

<form class="form-horizontal" role="form">
  <div class="form-group">
    <label for="inputEmail" class="col-sm-2 control-label">Email</label>
    <div class="col-sm-10">
      <input type="email" class="form-control" id="inputEmail" placeholder="Email" value="{{= email }}" name="email">
    </div>
  </div>
  <div class="form-group">
    <label for="inputPassword" class="col-sm-2 control-label">Password</label>
    <div class="col-sm-10">
      <input type="password" class="form-control" id="inputPassword" placeholder="Password" name="password">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="button" class="btn btn-default" id="sign_in_button">Sign In</button>
    </div>
  </div>
</form>  
<a class="btn btn-primary btn-lg" href="#register">Register</a>

<a href="#" onclick="facebookLogin()">Parse Facebook Login</a>-
</script>